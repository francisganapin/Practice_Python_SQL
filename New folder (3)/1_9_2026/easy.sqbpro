CREATE TABLE IF NOT EXISTS properties (
    property_id INTEGER PRIMARY KEY,
    address TEXT,
    city TEXT,
    bedrooms INTEGER,
    bathrooms REAL,
    price REAL,
    status TEXT
);

-- Agents table
CREATE TABLE IF NOT EXISTS agents (
    agent_id INTEGER PRIMARY KEY,
    name TEXT,
    phone TEXT
);

