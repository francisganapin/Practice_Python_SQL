-- select all properties
SELECT * FROM properties;


select address from properties;

select address,price from properties;

select * from properties where status = 'Available';


SELECT * from properties where city = 'Austin';

--properties where price was 400,000
select address,city,price
from properties 
where price &lt; 4000000;

select address, price FROM properties
ORDER BY price;

--Multiple Condition
SELECT address,city,price FROM properties
WHERE city = 'Austin' OR city = 'Houston';

--Number of Properties
SELECT COUNT(*) as number_of_properties from properties;

--Number of properties available
SELECT COUNT(*) as available_properteies
From properties
WHERE status = 'Available';



SELECT AVG(price) FROM properties;

-- Lowest prices 

SELECT 
	max(price) AS highest_price,
	min(price) AS lowest_price
FROM properties;

--property count by cities

SELECT city,AVG(price) AS avg_price
FROM properties
GROUP BY city
ORDER BY avg_price DESC;



--**Task:** Show properties priced between $300,000 and $500,000.
select * from properties
WHERE price BETWEEN 300000 AND  500000;


SELECT address,city,price FROM properties
WHERE city IN ('Austin','Dallas','Houston');

SELECT * FROM properties
WHERE address LIKE  '%oak%';

SELECT sum(price) as total_of_all_properties
from properties;

-- select properties where thier are more bedrooms 
SELECT * from properties where bedrooms &gt; 2;

SELECT address,price FROM properties
WHERE status = 'Available'
	and price &gt; (SELECT AVG(price) from properties);

--Count Properties by Status
SELECT  status,count(*) as status_count
from properties
group by status
order by status_count desc;
	

SELECT city, MAX(price) as highest_price
FROM properties
GROUP BY city
ORDER BY highest_price DESC;





	
	
